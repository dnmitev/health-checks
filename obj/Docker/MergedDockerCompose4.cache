services:
  sample:
    build:
      context: D:\health-checks
      dockerfile: D:\health-checks\Sample\Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      Logging__IncludeScopes: "false"
      Logging__LogLevel__Default: Debug
    healthcheck:
      test:
      - CMD-SHELL
      - curl -f http://sample/health
      timeout: 5s
      interval: 10s
      retries: 3
      start_period: 30s
    image: sample
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
networks:
  default:
    name: dockercompose8776384917650026233_default